<h1>Welcome to Twits</h1>
<%= form_for @user, :url => users_path, :html => { :multipart => true } do |form| %>
    <%= form.file_field :avatar %>
<% end %>
<%= form_for :twits, url: twits_path do |f| %>
  <p>
    <%= f.label :Twit %><br>
    <%= f.text_area :text %>
 </p>
 <p>
  <%= f.submit %>
 </p>
<% end %>
<%= render "welcome/twit" %>
